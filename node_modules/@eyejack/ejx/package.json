{
  "name": "@eyejack/ejx",
  "version": "0.8.4",
  "description": "Build interactive WebAR experiences on the EyeJackX platform.",
  "main": "./dist/ejx.umd.js",
  "module": "./dist/ejx.es.js",
  "bin": {
    "ejx": "./bin/index.mjs"
  },
  "files": [
    "./dist/",
    "./bin/",
    "./cli/"
  ],
  "exports": {
    ".": {
      "import": "./dist/ejx.es.js",
      "require": "./dist/ejx.umd.js"
    }
  },
  "scripts": {
    "dev": "concurrently \"npm run serve\" \"npm run watch\" \"npm run copy-trigger\"",
    "build": "vite build",
    "watch": "vite build --mode development -w",
    "serve": "http-server -S",
    "lint": "eslint 'src/' --ext js --fix",
    "test": "npm run lint",
    "prepare": "npm run build",
    "copy-trigger": "nodemon --watch dist --exec \"npm run copy-to-demos && npm run copy-to-editor\"",
    "copy-to-demos": "cp ./dist/* ../ejx-demos/libs/ejx/",
    "copy-to-editor": "cp ./dist/* ../ejx-threejs-editor/editor/js/libs/ejx/"
  },
  "author": "Jordan Santell <jsantell@google.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/node": "^18.13.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.31.0",
    "eslint-config-google": "^0.9.1",
    "glsl-y-hsv": "^1.0.0",
    "http-server": "^14.1.1",
    "nodemon": "^2.0.22",
    "rollup-plugin-visualizer": "^5.9.0",
    "typescript": "^4.9.5",
    "vite": "^4.0.4",
    "vite-plugin-externalize-deps": "^0.5.0"
  },
  "keywords": [
    "webar",
    "webvr",
    "webxr",
    "arcore",
    "arkit",
    "webaronarcore",
    "webaronarkit",
    "augmented reality"
  ],
  "dependencies": {
    "axios": "^1.3.2",
    "chalk": "^5.2.0",
    "clui": "^0.3.6",
    "figlet": "^1.5.2",
    "fs-extra": "^11.1.0",
    "gl-preserve-state": "^1.0.0",
    "inquirer": "^9.1.4",
    "jszip": "^3.10.1",
    "open": "^8.4.1",
    "preferences": "^2.0.2",
    "simple-git": "^3.16.0"
  },
  "peerDependencies": {
    "three": "^0.151.0"
  }
}
